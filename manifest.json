{
  "name": "Pachira",
  "description" : "Base Level - Pachira",
  "version": "1.0",
  "browser_action": {
    "default_popup": "hello.html",
    "default_icon": "icon-chrome.png"
  },
  "permissions": [
    "http://pachira.eba-zaetptb5.us-east-1.elasticbeanstalk.com/*",
    "https://www.google.com/*",
    "http://www.google.com/*",
    "http://127.0.0.1:8000/*",
    "http://localhost:8000/",
    "http://0.0.0.0:8000/*",
    "tabs",
    "activeTab",
    "https://www.amazon.com/*",
    "http://www.amazon.com/*",
    "geolocation",
    "storage",
    "identity"
  ],
  "manifest_version": 2,
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens hello.html"
    }
  }
}
